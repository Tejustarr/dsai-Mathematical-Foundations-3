   "source": [
    "import numpy as np\n",
    "X = np.array([[1,1],[1,2],[1,3]],float)\n",
    "y = np.array([2,2.5,3.5],float)\n",
    "w = np.linalg.inv(X.T@X)@X.T@y\n",
    "print('weights=', w)"
   ]
  },
