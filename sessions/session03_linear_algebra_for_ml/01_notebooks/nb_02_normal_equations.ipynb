   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3*w1**2 + 12*w1*w2 - 16.0*w1 + 14*w2**2 - 35.0*w2 + 22.5\n",
      "{w1: 1.16666666666667, w2: 0.750000000000000}\n"
     ]
    }
   ],
   "source": [
    "w1,w2=sp.symbols('w1 w2')\n",
    "X=sp.Matrix([[1,1],[1,2],[1,3]])\n",
    "y=sp.Matrix([2,2.5,3.5])\n",
    "w=sp.Matrix([w1,w2])\n",
    "L=(X*w-y).dot(X*w-y)\n",
    "print(sp.expand(L))\n",
    "print(sp.solve(sp.derive_by_array(L,(w1,w2)),(w1,w2)))"
   ]
  }
