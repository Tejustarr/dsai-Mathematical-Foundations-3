    "# Implement PCA function\n",
    "import numpy as np\n",
    "\n",
    "def pca(X,k):\n",
    "    Xc = X - X.mean(0)\n",
    "    U,S,Vt = np.linalg.svd(Xc,full_matrices=False)\n",
    "    return Xc@Vt[:k].T\n",
    "\n",
    "X=np.random.randn(100,5)\n",
    "Z=pca(X,2)\n",
    "print(Z.shape)"
   ]
